{
    "name": "lazywe/lazy-admin",
    "description": "基于laravel的rbac后台系统",
    "keywords": [
        "admin",
        "rbac",
        "laravel",
        "laravel admin"
    ],
    "authors": [
        {
            "name": "lazywe",
            "email": "lazywe@163.com"
        }
    ],
    "license": "MIT",
    "require": {
        "spatie/laravel-permission": "^3.3.0",
        "tormjens/eventy": "^0.5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Lazy\\Admin\\": "src/"
        },
        "files": [
            "src/tools.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lazy\\Admin\\ServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider",
                "TorMorten\\Eventy\\EventServiceProvider",
                "TorMorten\\Eventy\\EventBladeServiceProvider"
            ]
        }
    }
}